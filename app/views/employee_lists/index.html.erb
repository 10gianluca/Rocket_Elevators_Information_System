<p id="notice"><%= notice %></p>

<h1>Employee Lists</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Title</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @employee_lists.each do |employee_list| %>
      <tr>
        <td><%= employee_list.first_name %></td>
        <td><%= employee_list.last_name %></td>
        <td><%= employee_list.title %></td>
        <td><%= employee_list.email %></td>
        <td><%= link_to 'Show', employee_list %></td>
        <td><%= link_to 'Edit', edit_employee_list_path(employee_list) %></td>
        <td><%= link_to 'Destroy', employee_list, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Employee List', new_employee_list_path %>
