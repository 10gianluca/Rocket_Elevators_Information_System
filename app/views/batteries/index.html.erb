<p id="notice"><%= notice %></p>

<h1>Batteries</h1>

<table>
  <thead>
    <tr>
      <th>Type</th>
      <th>Status</th>
      <th>Employeeid</th>
      <th>Dateofcomissioning</th>
      <th>Dateoflastinspection</th>
      <th>Certificationofops</th>
      <th>Information</th>
      <th>Notes</th>
      <th>Building</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @batteries.each do |battery| %>
      <tr>
        <td><%= battery.type %></td>
        <td><%= battery.status %></td>
        <td><%= battery.employeeId %></td>
        <td><%= battery.dateofcomissioning %></td>
        <td><%= battery.dateoflastinspection %></td>
        <td><%= battery.certificationofops %></td>
        <td><%= battery.information %></td>
        <td><%= battery.notes %></td>
        <td><%= battery.building %></td>
        <td><%= link_to 'Show', battery %></td>
        <td><%= link_to 'Edit', edit_battery_path(battery) %></td>
        <td><%= link_to 'Destroy', battery, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Battery', new_battery_path %>
