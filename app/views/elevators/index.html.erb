<p id="notice"><%= notice %></p>

<h1>Elevators</h1>

<table>
  <thead>
    <tr>
      <th>Columnid</th>
      <th>Serialnumber</th>
      <th>Model</th>
      <th>Type</th>
      <th>Status</th>
      <th>Commissioningdate</th>
      <th>Lastinspection</th>
      <th>Inspectioncertificate</th>
      <th>Information</th>
      <th>Notes</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @elevators.each do |elevator| %>
      <tr>
        <td><%= elevator.columnID %></td>
        <td><%= elevator.serialNumber %></td>
        <td><%= elevator.model %></td>
        <td><%= elevator.type %></td>
        <td><%= elevator.status %></td>
        <td><%= elevator.commissioningDate %></td>
        <td><%= elevator.lastInspection %></td>
        <td><%= elevator.inspectionCertificate %></td>
        <td><%= elevator.information %></td>
        <td><%= elevator.notes %></td>
        <td><%= link_to 'Show', elevator %></td>
        <td><%= link_to 'Edit', edit_elevator_path(elevator) %></td>
        <td><%= link_to 'Destroy', elevator, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Elevator', new_elevator_path %>
